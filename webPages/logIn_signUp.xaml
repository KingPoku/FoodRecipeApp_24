<Page x:Class="FoodRecipeApp_24.logIn_signUp"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:FoodRecipeApp_24"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="logIn_signUp">

    <Grid Background="White" RenderTransformOrigin="0.5,0.5">

        <Button x:Name="Create_an_account" Content="Create a new account" HorizontalAlignment="Left" Height="60" Margin="267,172,0,0" VerticalAlignment="Top" Width="261" Background="#FF918F8F" FontSize="20" FontWeight="Bold" Click="Create_an_account_Click"/>
        <Button x:Name="Create_an_account_Copy" Content="Log In" HorizontalAlignment="Left" Height="60" Margin="268,257,0,0" VerticalAlignment="Top" Width="261" Background="#FF918F8F" FontSize="20" FontWeight="Bold" Click="Create_an_account_Click"/>

    </Grid>
</Page>
